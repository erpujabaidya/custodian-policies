{
  "policy": {
    "name": "sg-add-permission",
    "resource": "security-group",
    "description": "Filter any security group that\nallows 0.0.0.0/0 or ::/0 (IPv6) ingress on port 22, remove\nthe rule\n",
    "mode": {
      "type": "cloudtrail",
      "role": "arn:aws:iam::364604277807:role/Custodian-Policy",
      "events": [
        {
          "source": "ec2.amazonaws.com",
          "event": "AuthorizeSecurityGroupIngress",
          "ids": "requestParameters.groupId"
        },
        {
          "source": "ec2.amazonaws.com",
          "event": "RevokeSecurityGroupIngress",
          "ids": "requestParameters.groupId"
        }
      ],
      "tags": {
        "custodian-info": "mode=cloudtrail:version=0.9.10"
      }
    },
    "filters": [
      {
        "or": [
          {
            "type": "ingress",
            "Ports": [
              22
            ],
            "Cidr": "0.0.0.0/0"
          },
          {
            "type": "ingress",
            "Ports": [
              22
            ],
            "CidrV6": "::/0"
          }
        ]
      }
    ]
  },
